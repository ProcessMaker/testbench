{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "name": {
                "type": "string",
                "minLength": 1
            },
            "url": {
                "type": "string",
                "format": "uri"
            },
            "bearerToken": {
                "type": "string",
                "minLength": 1
            }
        },
        "required": [
            "name",
            "url",
            "bearerToken"
        ],
        "additionalProperties": false
    }
}